{
  "name": "ana",
  "version": "0.0.0",
  "description": "An analytics Django app",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf static/build",
    "prebuild": "npm run clean",
    "build": "webpack",
    "prebuild:watch": "npm run clean",
    "build:watch": "webpack --watch",
    "lint": "eslint 'ana/static/ana/**/*.jsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emanlodovice/ana.git"
  },
  "keywords": [
    "analytics"
  ],
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emanlodovice/ana/issues"
  },
  "homepage": "https://github.com/emanlodovice/ana#readme",
  "dependencies": {
    "react": "16.8.1",
    "react-dom": "16.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "eslint": "5.13.0",
    "eslint-config-arnellebalane": "5.0.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint"
  }
}
